{
    "all": true,
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/**/*.test.ts",
        "src/test/**/*",
        "dist/**/*",
        "out/**/*"
    ],
    "extension": [
        ".ts"
    ],
    "require": [
        "ts-node/register"
    ],
    "reporter": [
        "text",
        "html",
        "json",
        "lcov"
    ],
    "sourceMap": true,
    "instrument": true
}