{
	"name": "ccs-flutter-color",
	"displayName": "Flutter Color",
	"description": "This plugin help you to visualize the colors used in your project.",
	"version": "2.1.0",
	"publisher": "circlecodesolution",
	"keywords": [
		"futlter",
		"dart",
		"color",
		"android",
		"ios",
		"yml",
		"json"
	],
	"author": {
		"name": "Nilesh Chavan"
	},
	"icon": "images/logo-color.jpg",
	"galleryBanner": {
		"color": "#356bbc",
		"theme": "dark"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/neodisk17/vscode-ext-flutter-color-viewer/issues",
		"email": "lk07540@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/neodisk17/vscode-ext-flutter-color-viewer"
	},
	"engines": {
		"vscode": "^1.81.0"
	},
	"categories": [
		"Other"
	],
	"languages": [
		{
			"id": "dart",
			"extensions": [
				".dart",
				".cs",
				".js",
				".json"
			],
			"aliases": [
				"dart",
				"cs",
				"js",
				"json"
			]
		}
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": {
			"title": "Flutter Color",
			"type": "object",
			"properties": {
				"flutterColor.hexFormat": {
					"type": "string",
					"enum": [
						"lower case",
						"upper case"
					],
					"default": "upper case",
					"description": "Case for letters in hex strings"
				},
				"flutterColor.includeAlpha": {
					"type": "boolean",
					"default": true,
					"description": "Include alpha channel in color preview"
				}
			}
		},
		"commands": []
	},
	"homepage": "https://github.com/neodisk17/vscode-ext-flutter-color-viewer/blob/master/README.md",
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/sinon": "^17.0.3",
		"@types/vscode": "^1.81.0",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^8.47.0",
		"glob": "^10.5.0",
		"mocha": "^10.2.0",
		"sinon": "^18.0.0",
		"ts-loader": "^9.4.4",
		"typescript": "^5.1.6",
		"webpack": "^5.94.0",
		"webpack-cli": "^5.1.4"
	},
	"announcement": {
		"onVersion": "2.0.0",
		"message": "[NEW] Refactor the code, Added Test cases, Implemented Telementry & crash reporting"
	}
}
