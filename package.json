{
	"name": "ccs-flutter-color",
	"displayName": "Flutter Color",
	"description": "This plugin help you to easily visualize the color.",
	"version": "1.2.5",
	"publisher": "circlecodesolution",
	"keywords": [
		"futlter",
		"dart",
		"color",
		"andriod",
		"ios"
	],
	"author": {
		"name": "Nilesh Chavan"
	},
	"icon": "images/logo-color.jpg",
	"galleryBanner": {
		"color": "#356bbc",
		"theme": "dark"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/neodisk17/vscode-ext-flutter-color-viewer/issues",
		"email": "lk07540@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/neodisk17/vscode-ext-flutter-color-viewer"
	},
	"engines": {
		"vscode": "^1.81.0"
	},
	"categories": [
		"Other"
	],
	"languages": [
		{
			"id": "dart",
			"extensions": [
				".dart",
				".cs",
				".js",
				".json"
			],
			"aliases": [
				"dart",
				"cs",
				"js",
				"json"
			]
		}
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": []
	},
	"homepage": "https://github.com/neodisk17/vscode-ext-flutter-color-viewer/blob/master/README.md",
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"lint:fix": "eslint src --ext ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\""
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.2",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/sinon": "^17.0.3",
		"@types/vscode": "^1.81.0",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"glob": "^10.3.3",
		"mocha": "^10.2.0",
		"prettier": "^2.7.1",
		"sinon": "^18.0.0",
		"ts-loader": "^9.4.4",
		"typescript": "^5.1.6",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	},
	"announcement": {
		"onVersion": "1.0.1",
		"message": "[NEW] Integration  of Color palette: supports all the languages except - CSS, LESS, SCSS."
	}
	
}
