{
	"name": "ccs-flutter-color",
	"displayName": "Flutter Color",
	"description": "This plugin help you to easily visualize the color.",
	"version": "1.0.0",
	"publisher": "circlecodesolution",
	"keywords": [
		"futlter",
		"dart",
		"color",
		"andriod",
		"ios"
	],
	"author": {
		"name": "Nilesh Chavan"
	},
	"icon": "images/logo-color.jpg",
	"galleryBanner": {
		"color": "#356bbc",
		"theme": "dark"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/circlecodesolution/vscode-ext-flutter-color-viewer/issues",
		"email": "codesolution.in@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/circlecodesolution/vscode-ext-flutter-color-viewer.git"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"languages": [
		{
			"id": "dart",
			"extensions": [
				".dart",
				".cs",
				".js",
				".json"
			],
			"aliases": [
				"dart",
				"cs",
				"js",
				"json"
			]
		}
	],
	"activationEvents": [
		"workspaceContains:**/*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"homepage": "https://github.com/circlecodesolution/vscode-ext-flutter-color-viewer/blob/master/README.md",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^8.1.3",
		"serialize-javascript": ">=3.1.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.9"
	},
	"announcement": {
		"onVersion": "1.0.0",
		"message": "[NEW] Intregation of Color palette: supports all the languages except - CSS, LESS, SCSS."
	}
}
